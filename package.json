{
  "name": "expense-tracker-pro",
  "version": "1.0.0",
  "description": "Tracker avanzato per abbonamenti e spese mensili con gestione CSV",
  "main": "server.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "setup": "npm install && mkdir -p data public/src && echo 'Setup completato! Usa npm start per avviare l\\'app'",
    "dev": "npm start"
  },
  "keywords": [
    "expense-tracker",
    "subscription-manager",
    "csv",
    "react",
    "express",
    "financial-tracker"
  ],
  "author": "Expense Tracker Pro",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "lucide-react": "^0.263.1",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.8.0",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/expense-tracker-pro.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/expense-tracker-pro/issues"
  },
  "homepage": "."
}
